        android:visibility="gone">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="6"
                android:background="@color/white"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/default_space_32"
                    android:layout_marginTop="@dimen/default_space_48"
                    android:layout_marginEnd="@dimen/default_space_32"
                    android:src="@drawable/err_locationxxhdpi" />


            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:background="@color/white">


                <TextView
                    android:id="@+id/tv_show_location_wrong"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView27"
                    android:layout_alignParentStart="false"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="@dimen/size_16_dp"
                    android:text="@string/failed_to_verify_your_exact_location_please_retry"
                    android:textAlignment="center"
                    android:textColor="@color/sub_heading_color"
                    android:visibility="visible" />


                <Button
                    android:id="@+id/btn_fetch_location_retry_start_anyway"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="0dp"
                    android:background="@drawable/gradient_6"
                    android:text="@string/start_anyway"
                    android:textAlignment="center"
                    android:textColor="@color/white" />

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/default_space_32"
                    android:text="@string/oops"
                    android:textColor="@color/primary_dark_app"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView21"
                    android:layout_alignParentTop="false"
                    android:layout_alignParentEnd="false"
                    android:layout_centerInParent="false"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/size_8_dp"
                    android:text="@string/seems_like_you_re_flying"
                    android:textColor="@color/primary_dark_app"
                    android:textSize="24sp" />

            </RelativeLayout>


        </LinearLayout>


    </FrameLayout>


